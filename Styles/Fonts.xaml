<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:StatFeed">
    
    <!-- Naming conventions are not what you expect-->
    <!-- Overpass Light is called "Overpass Light" -->
    <!-- Overpass Bold is called "Overpass"-->
    <!-- Overpass Extra Bold is called "Overpass ExtraBold-->

    <FontFamily x:Key="OverpassLight">pack://appliction;,,,/Fonts/#Overpass Light</FontFamily>
    <FontFamily x:Key="OverpassBold">pack://appliction;,,,/Fonts/#Overpass</FontFamily>
    <FontFamily x:Key="OverpassExtraBold">pack://appliction;,,,/Fonts/#Overpass ExtraBold</FontFamily>
    
    <FontFamily x:Key="DotMatrix">pack://appliction;,,,/Fonts/#5x7 DOT Matrix </FontFamily>
    <FontFamily x:Key="VcrMono">pack://appliction;,,,/Fonts/#VCR OSD Mono</FontFamily>
    <FontFamily x:Key="Pixel_FJ">pack://appliction;,,,/Fonts/#pixelFJ8pt1</FontFamily>



</ResourceDictionary>